{
  "name": "angular2-fullstack-starter",
  "version": "0.0.1",
  "description": "A skeleton application with Angular 2 (ES6/Babel), SystemJS, Express and Node",
  "main": "index.js",
  "scripts": {
    "build:cache": "express-compile public",
    "lint": "eslint public server test",
    "prepublish": "npm run lint && npm run build:cache",
    "preprotractor": "npm run webdriver-update",
    "protractor": "protractor test/protractor.conf.js",
    "start": "node index.js",
    "test": "karma start test/karma.conf.js",
    "test:watch": "karma start test/karma.conf.js --single-run false --auto-watch true",
    "webdriver-update": "./node_modules/protractor/bin/webdriver-manager update"
  },
  "author": "Joshua Godi <joshuagodi@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/jgodi/angular2-fullstack-starter.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jgodi/angular2-fullstack-starter/issues"
  },
  "homepage": "https://github.com/jgodi/angular2-fullstack-starter",
  "dependencies": {
    "angular2": "2.0.0-beta.1",
    "angular2-library-starter": "^0.0.6",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.0",
    "supports-color": "^3.1.2",
    "systemjs": "0.19.15",
    "zone.js": "0.5.10"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.17",
    "babel-eslint": "^4.1.6",
    "babel-plugin-angular2-annotations": "^5.0.0",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-decorators-legacy": "^1.3.3",
    "babel-plugin-transform-flow-strip-types": "^6.3.15",
    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "babylon": "6.3.21",
    "body-parser": "^1.14.1",
    "browserify": "^13.0.0",
    "chalk": "^1.1.1",
    "convict": "^1.0.1",
    "cors": "^2.7.1",
    "ejs": "^2.3.4",
    "eslint": "^1.10.3",
    "eslint-config-bullhorn": "0.0.12",
    "express": "^4.13.3",
    "express-compile": "^3.0.0",
    "express-compilers": "^3.0.1",
    "express-session": "^1.12.1",
    "ghooks": "^1.0.1",
    "jasmine-core": "^2.4.1",
    "jasmine-spec-reporter": "^2.4.0",
    "karma": "^0.13.19",
    "karma-browserify": "^4.4.2",
    "karma-jasmine": "^0.3.6",
    "karma-mocha-reporter": "^1.1.5",
    "karma-phantomjs-launcher": "^0.2.3",
    "karma-redirect-preprocessor": "^0.1.1",
    "karma-source-map-support": "^1.1.0",
    "morgan": "^1.6.1",
    "pathmodify": "^0.5.0",
    "phantomjs": "^1.9.19",
    "protractor": "^3.0.0"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint"
    }
  }
}
